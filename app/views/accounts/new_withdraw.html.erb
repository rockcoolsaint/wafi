<h1>Withdrawal</h1>
<h3>balance: <%= @account.balance %></h3>
<%= form_with url: withdraw_user_account_path, local: true do |form| %>

  <div class="field">
    <%= form.label :amount %>
    <%= form.number_field :amount %>
  </div>

  <div class="actions">
    <%= form.submit 'Withdraw' %>
  </div>
<% end %>

