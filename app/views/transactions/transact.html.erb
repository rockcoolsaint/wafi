<h1>Transact</h1>

<%= form_with do |form| %>

  <div class="field">
    <%= form.label :account_number %>
    <%= form.number_field :account_number %>
  </div>

  <div class="field">
    <%= form.label :balance %>
    <%= form.text_field :balance %>
  </div>

  <div class="field">
    <%= form.label :user_id %>
    <%= form.text_field :user_id %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>

